<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ProfessorProfileViewController">
   <children>
      <Button fx:id="mainPageButton" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#returnToMainPage" text="mainPage" />
      <ImageView fx:id="photo" fitHeight="150.0" fitWidth="121.0" layoutX="34.0" layoutY="68.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="changeThemeButton" layoutX="516.0" layoutY="24.0" mnemonicParsing="false" onAction="#setChangeThemeButton" text="changeTheme" />
      <TextField fx:id="mailTextField" layoutX="195.0" layoutY="85.0" onMouseDragExited="#updateMail" />
      <TextField fx:id="phoneNumberTextField" layoutX="195.0" layoutY="130.0" onMouseDragExited="#updatePhoneNumber" />
      <Label fx:id="nationalNumberLabel" layoutX="195.0" layoutY="174.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="facultyLabel" layoutX="38.0" layoutY="244.0" prefHeight="25.0" prefWidth="121.0" text="Label" />
      <Label fx:id="professorNumberLabel" layoutX="38.0" layoutY="282.0" prefHeight="25.0" prefWidth="121.0" text="Label" />
      <Label fx:id="degreeLabel" layoutX="388.0" layoutY="155.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="roomIDLabel" layoutX="388.0" layoutY="104.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="nameLabel" layoutX="228.0" layoutY="24.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Button layoutX="252.0" layoutY="110.0" mnemonicParsing="false" onAction="#updateMail" text="ثبت">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
      <Button layoutX="252.0" layoutY="158.0" mnemonicParsing="false" onAction="#updatePhoneNumber" text="ثبت">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
