<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ProfessorCourseListController">
   <children>
      <TextField fx:id="facultyTextbox" layoutX="78.0" layoutY="88.0" prefHeight="22.0" prefWidth="97.0" promptText="Faculty" />
      <TextField fx:id="sectionTextbox" layoutX="78.0" layoutY="171.0" prefHeight="22.0" prefWidth="97.0" promptText="Course Section" />
      <TextField fx:id="numberTextbox" layoutX="78.0" layoutY="129.0" prefHeight="22.0" prefWidth="97.0" promptText="Course Number" />
      <Button fx:id="filterButton" layoutX="101.0" layoutY="222.0" mnemonicParsing="false" onAction="#Filter" text="Filter" />
      <Button fx:id="mainPageButton" layoutX="24.0" layoutY="20.0" mnemonicParsing="false" onAction="#returnToMainPage" text="MainPage" />
      <ListView fx:id="courseListView" layoutX="243.0" layoutY="10.0" prefHeight="380.0" prefWidth="320.0" />
      <Button fx:id="addButton" layoutX="14.0" layoutY="321.0" mnemonicParsing="false" onAction="#creatNewCourse" prefHeight="65.0" prefWidth="78.0" text="add new course">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" layoutX="153.0" layoutY="360.0" mnemonicParsing="false" onAction="#deleteCourse" prefHeight="25.0" prefWidth="43.0" text="delete">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
      <TextField fx:id="deleteCourseNumber" layoutX="126.0" layoutY="321.0" prefHeight="25.0" prefWidth="97.0" promptText="course number " />
   </children>
</AnchorPane>
