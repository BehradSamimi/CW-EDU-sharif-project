<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.StudentProfileViewController">
   <children>
      <Button fx:id="mainPageButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnToMainPage" text="mainPage" />
      <ImageView fx:id="photo" fitHeight="150.0" fitWidth="121.0" layoutX="24.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="changeThemeButton" layoutX="506.0" layoutY="14.0" mnemonicParsing="false" onAction="#setChangeThemeButton" text="changeTheme" />
      <TextField fx:id="mailTextField" layoutX="185.0" layoutY="75.0" onInputMethodTextChanged="#updateMail" />
      <TextField fx:id="phoneNumberTextField" layoutX="185.0" layoutY="120.0" onMouseDragExited="#updatePhoneNumber" />
      <Label fx:id="nationalNumberLabel" layoutX="185.0" layoutY="164.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="facultyLabel" layoutX="28.0" layoutY="228.0" prefHeight="25.0" prefWidth="121.0" text="Label" />
      <Label fx:id="studentNumberLabel" layoutX="28.0" layoutY="268.0" prefHeight="25.0" prefWidth="121.0" text="Label" />
      <Label fx:id="GpaLabel" layoutX="28.0" layoutY="311.0" prefHeight="25.0" prefWidth="121.0" text="Label" />
      <Label fx:id="sectionLabel" layoutX="371.0" layoutY="75.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="entranceYearLabel" layoutX="371.0" layoutY="120.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="educationalStateLabel" layoutX="371.0" layoutY="164.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="nameLabel" layoutX="225.0" layoutY="26.0" prefHeight="25.0" prefWidth="149.0" text="Label" />
      <Label fx:id="superVisorLabel" layoutX="28.0" layoutY="352.0" prefHeight="25.0" prefWidth="121.0" text="Label" />
      <Button fx:id="recordMail" layoutX="234.0" layoutY="100.0" mnemonicParsing="false" onAction="#updateMail" prefHeight="15.0" prefWidth="52.0" text="ثبت">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
      <Button fx:id="recordPhone" layoutX="234.0" layoutY="145.0" mnemonicParsing="false" onAction="#updatePhoneNumber" prefHeight="15.0" prefWidth="52.0" text="ثبت">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
